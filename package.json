{
  "name": "react-redux-firebase",
  "version": "0.0.1",
  "description": "A boilerplate using Webpack, React, Redux and Firebase",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.babel.js",
    "dev-old": "cross-env NODE_ENV=development node server-dev.es5.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config webpack.config.dev.js",
    "dev:w": "npm run dev -- -w"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.7.2",
    "cross-env": "^2.0.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "firebase": "^3.2.0",
    "html-webpack-plugin": "^2.22.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.4.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vkammerer/react-redux-firebase.git"
